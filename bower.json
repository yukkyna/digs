  "dependencies": {
    "jquery": "~2.1.1",
    "normalize.css": "~3.0.1",
    "bootstrap": "~3.2.0",
    "tinymce": "~4.1.4",
    "select2": "~3.5.1",
    "select2-bootstrap3-css": "~1.4.1"
  }
